{
   "background": {
      "persistent": false,
      "scripts": [ "js/jquery/jquery-1.10.2.min.js"]
   },
   "browser_action": {
      "default_icon": "assets/icon19.png",
      "default_popup": "html/popup.html",
      "default_title": "NTULamsBeautifier"
   },
   "content_scripts": [ {
      "css": [ "css/core.css" ],
      "js": [ "js/core.js", "js/common/settings.js" ],
      "matches": [ "*://https://lams.ntu.edu.sg/lams/tool/*"],
      "run_at": "document_end"
   } ],
   "default_locale": "en",
   "description": "__MSG_extension_description__",
   "externally_connectable": {
      "matches": [ "*://ntulearn.ntu.edu.sg/*", "*://lams.ntu.edu.sg/*" ]
   },
   "homepage_url": "#",
   "icons": {
      "128": "assets/icon128.png",
      "16": "assets/icon16.png",
      "19": "assets/icon19.png",
      "48": "assets/icon48.png"
   },
   "manifest_version": 2,
   "name": "NTU Lams Beautifier",
   "options_page": "html/popup.html",
   "permissions": [ "notifications", "alarms", "storage", "background", "webRequest", "webRequestBlocking", "*://lams.ntu.edu.sg/lams/tool/*"],
   "short_name": "LamsB",
   "update_url": "https://localhost",
   "version": "0.01",
   "web_accessible_resources": [ "_locales/*", "bundle/*", "assets/*", "font/*", "html/bookmarks.html", "css/*.css", "js/*.js", "js/jquery/*.js", "js/lang/*" ]
}
